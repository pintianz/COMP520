class Pass03 {

	public static void main(String[] args) {
		Pass03 p3 = new Pass03();
		p3.f();
	}

	public void f() {
		C = 5;
		System.out.println(C);
	}

	int C;  // hides class C at member level

}

class C {
	int x;
}


