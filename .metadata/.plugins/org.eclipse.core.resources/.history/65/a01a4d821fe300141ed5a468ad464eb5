/**
 * COMP 520
 * Instance values
 */
class MainClass {
	int x;
    public static void main (String [] args) {
    	MainClass mc = new MainClass();
    	mc.x = -5;
        if(!false){
        	System.out.println(mc.x);
        }
    }
    
    public boolean b1 () {
    	x=6;
    	return false;
    }
    public boolean b2 () {
    	x=7;
    	return true;
    }
}
